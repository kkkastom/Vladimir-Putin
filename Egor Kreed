#include<stdio.h>
#include<math.h>
  
int main(){

  printf("Vvedite koeficienty:");
  double D = 0, a = 0, b = 0, c = 0;

  scanf("%lf %lf %lf", &a, &b, &c);
    D = (b * b) - (4 * a *c);


    if (D >= 1) {
      printf("Kornei vsego: 2\n x1 = %.0f, x2 = %.0f\n", (-b + sqrt(D)) / 2 * a, (-b - sqrt(D)) / 2 * a );
  } else {
    if (D == 0) {
      printf("Kornei vsego: 1\n x = %.0f\n", -b / (2 * a) ); 
  } else {
    if (D <= -1) {
      printf("Kornei net\n");
  }}
  }

  return (0);

}
